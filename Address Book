<script type ="text/javascript">

function user (firstname,lastname,email){
	this.firstname = firstname;
	this.lastname = lastname;
	this.email = email;}
	
var person1 = new user ("Samantha", "Houdek", "samhoudek@gmail.com");
var person2 = new user ("Jess", "Rush", "rush@example.com");

var contacts = [person1, person2];

function printPerson(user) {
    console.log(user.firstName + " " + user.lastName+ "'s"+ " " +"email" + " " + "is" + " " + user.email);
}

var list = function(){
    var contactsLength = contacts.length; 
    for( i = 0; i < contactsLength; i++){
        printPerson(contacts[i]);
    }

var add = function(firstName,lastName,email,phoneNumber){
contacts[contacts.length]= {
firstName: firstName,
lastName: lastName,
email: email,
};	
};
